<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<!--
    TestNG Suite Configuration
    Generated by the Contract-Driven API Automation Engine

    HOW TO RUN:
      mvn test                          (uses this file via surefire plugin)
      mvn test -Dgroups=smoke           (run only tests tagged @Test(groups="smoke"))

    HOW TO ADD MORE TEST CLASSES:
      Add a <class name="..."/> entry inside the <classes> element below.
-->
<suite name="{{suiteName}}" verbose="1" parallel="false">

    <test name="{{testName}}">
        <classes>
            {{#testClasses}}
            <class name="{{.}}"/>
            {{/testClasses}}
        </classes>
    </test>

</suite>
